{
  "kind": "collectionType",
  "collectionName": "images",
  "info": {
    "name": "Image"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "imageID": {
      "type": "string",
      "required": true
    },
    "alt": {
      "type": "string",
      "required": true
    },
    "objectPosition": {
      "type": "string",
      "required": true
    },
    "animationDelay": {
      "type": "integer",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    }
  }
}
